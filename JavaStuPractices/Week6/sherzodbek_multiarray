public class java {
    public static void main(String[] args) {
        int[][] matrix={{1,2,3,2,3,2,1}, {4,5,6,2,1,2,7}};
        for (int i=0; i<matrix.length; i++){
            for(int j=0; j<matrix[i].length; j++){
                System.out.print(matrix[i][j]);
            }
        }
        
    }
}
